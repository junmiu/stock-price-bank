{
  "name": "stock-price-bank",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"nodemon --watch src/graphql/schema.graphql --watch dist --exec \\\"npm start\\\"\"",
    "start": "node dist/server.js",
    "prestart": "npm run clean && npm run build && npm run copy:schema",
    "build": "tsc",
    "clean": "rimraf dist",
    "copy:schema": "cp src/graphql/schema.graphql dist/graphql/schema.graphql",
    "seed": "knex seed:run",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "graphql": "^16.10.0",
    "graphql-http": "^1.22.4",
    "knex": "^2.5.1",
    "mongodb": "^4.17.2",
    "sqlite3": "^5.1.6"
  }
}
